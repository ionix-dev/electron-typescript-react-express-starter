{
	"name": "starter",
	"version": "1.0.0",
	"description": "starter",
	"author": "ionix.dev",
	"build": {
		"productName": "starter",
		"appId": "org.ionix.starter",
		"files": [
			"build/**/*"
		],
		"extraResources": [],
		"directories": {
			"buildResources": "server/electron/assets",
			"output": "dist"
		},
		"publish": {
			"provider": "github",
			"owner": "ionix.dev",
			"repo": "https://github.com/ionix-dev",
			"private": false
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		},
		"win": {
			"icon": "server/electron/assets/win/icon.png",
			"target": [
				"nsis"
			]
		}
	},
	"dependencies": {
		"@material-ui/core": "^4.11.3",
		"@material-ui/icons": "^4.11.2",
		"@material-ui/lab": "^4.0.0-alpha.57",
		"@reduxjs/toolkit": "^1.6.0",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"axios": "^0.21.1",
		"bcrypt": "^5.0.0",
		"bytenode": "^1.2.2",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"dotenv": "^10.0.0",
		"eccrypto": "^1.1.5",
		"electron-is-dev": "^2.0.0",
		"electron-store": "^7.0.3",
		"eslint-config-react-app": "^6.0.0",
		"express": "^4.17.1",
		"hddserial": "^0.0.7",
		"install": "^0.13.0",
		"joi": "^17.3.0",
		"js-search": "^2.0.0",
		"jsonwebtoken": "^8.5.1",
		"jspdf": "^2.3.1",
		"jspdf-autotable": "^3.5.14",
		"moment": "^2.29.1",
		"nodemon": "^2.0.6",
		"react": "^17.0.2",
		"react-barcode": "^1.4.0",
		"react-barcode-reader": "^0.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.4",
		"react-router-dom": "^5.2.0",
		"react-scripts": "4.0.3",
		"react-transition-group": "^4.4.1",
		"react-virtualized": "^9.22.3",
		"reflect-metadata": "^0.1.13",
		"sequelize": "^6.3.5",
		"sequelize-typescript": "^2.0.0-beta.1",
		"serial-number": "^1.3.0",
		"sqlite3": "^5.0.0",
		"uuid": "^8.3.2",
		"web-vitals": "^1.0.1"
	},
	"scripts": {
		"front": "react-scripts start",
		"server": "cd server && tsc-watch --project . --outDir ../build/server --onSuccess \"nodemon ../build/server/api/app.js\"",
		"web": "concurrently \"yarn server\" \"yarn front\"",
		"watch-electron": "cd server && tsc-watch --project . --outDir ../build/server --onSuccess \"electron ../build/server/electron.js\"",
		"electron": "concurrently \"cross-env BROWSER=none yarn front\" \"wait-on http://localhost:50501 && yarn watch-electron\"",
		"build-front": "cross-env PUBLIC_URL=./&& react-scripts build",
		"build-server": "cd server && tsc -p tsconfig.json",
		"build": "yarn build-front && yarn build-server",
		"obfuscate": "javascript-obfuscator ./build/server/electron/main.js --string-array true --string-array-encoding rc4 --output ./build/server/electron/main.js --compact true --self-defending false",
		"build:compiler": "cd jsc-compile && tsc -p tsconfig.json",
		"compile": "yarn build && yarn obfuscate && yarn build:compiler && electron ./jsc-compile/compile.js",
		"electron:pack": "yarn compile && electron-builder --windows --x64"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@types/bcrypt": "^3.0.1",
		"@types/bluebird": "^3.5.33",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.9",
		"@types/eccrypto": "^1.1.2",
		"@types/express": "^4.17.11",
		"@types/faker": "^5.1.5",
		"@types/jest": "^26.0.15",
		"@types/joi": "^14.3.4",
		"@types/js-search": "^1.4.0",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/material-ui": "^0.21.8",
		"@types/node": "^14.14.20",
		"@types/react": "^17.0.2",
		"@types/react-dom": "^17.0.0",
		"@types/react-router-dom": "^5.1.7",
		"@types/react-transition-group": "^4.4.1",
		"@types/react-virtualized": "^9.21.11",
		"@types/sequelize": "^4.28.9",
		"@types/sqlite3": "^3.1.6",
		"@types/uuid": "^8.3.0",
		"@types/validator": "^13.1.3",
		"@typescript-eslint/eslint-plugin": "^4.13.0",
		"@typescript-eslint/parser": "^4.13.0",
		"babel-loader": "8.1.0",
		"concurrently": "^5.3.0",
		"cross-env": "^7.0.3",
		"electron": "^12.0.2",
		"electron-builder": "^22.10.5",
		"eslint": "^7.17.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"faker": "^5.3.1",
		"javascript-obfuscator": "^2.12.0",
		"ts-node": "^9.1.1",
		"ts-node-dev": "^1.1.6",
		"tsc-watch": "^4.4.0",
		"typescript": "^4.1.3",
		"wait-on": "^5.2.1"
	}
}
